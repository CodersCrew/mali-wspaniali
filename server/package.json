{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest  --detectOpenHandles --runInBand --forceExit",
    "test:watch": "jest --watch --detectOpenHandles --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env NODE_ENV=test jest --runInBand --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/gateway": "^0.17.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/cqrs": "^7.0.0",
    "@nestjs/graphql": "7.9.1",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "7.2.4",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.0",
    "@sentry/node": "^5.19.0",
    "apollo-server-express": "^2.15.0",
    "axios": "^0.21.2",
    "bcrypt": "5.0.0",
    "class-transformer": "0.3.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "0.12.2",
    "graphql": "^15.1.0",
    "graphql-tools": "^6.0.10",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.6.0",
    "icg-json-to-xlsx": "^0.2.5",
    "lodash.pick": "^4.4.0",
    "module-alias": "^2.2.2",
    "mongoose": "5.11.15",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "shallow-equal-object": "^1.1.1",
    "short-unique-id": "^3.0.3",
    "type-graphql": "^1.0.0-rc.2",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "^26.0.21",
    "@types/node": "^13.9.1",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.6.3",
    "jest-serial-runner": "^1.1.0",
    "mockdate": "^3.0.3",
    "mongodb-memory-server": "6.9.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": [
      ".spec.ts$",
      "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "runner": "jest-serial-runner",
    "globalSetup": "./setup.ts",
    "globalTeardown": "./teardown.ts",
    "moduleNameMapper": {
      "^@users/(.*)$": "<rootDir>/users/$1",
      "^@kindergartens/(.*)$": "<rootDir>/kindergartens/$1",
      "^@keyCodes/(.*)$": "<rootDir>/key_codes/$1",
      "^@articles/(.*)$": "<rootDir>/articles/$1",
      "^@app/(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../setupTests.ts"
    ]
  },
  "_moduleAliases": {
    "@users": "dist/users",
    "@kindergartens": "dist/kindergartens",
    "@keyCodes": "dist/key_codes",
    "@articles": "dist/articles",
    "@app": "dist"
  }
}
